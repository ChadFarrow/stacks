{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "strict": true,
    "jsx": "preserve",
    "sourceMap": true,
    "resolveJsonModule": true,
    "importHelpers": true,
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "baseUrl": ".",
    "types": [
      "node",
      "vitest",
      "vite/client"
    ],
    "paths": {
      "stacks": [
        "./.stacks/index.ts"
      ],
      "~/functions": [
        "./functions/index.ts"
      ],
      "stacks/*": [
        "./.stacks/src/*"
      ],
      "builds": [
        "./.stacks/src/builds/index.ts"
      ],
      "components": [
        "./components/index.ts"
      ],
      "config": [
        "./config/index.ts"
      ],
      "functions": [
        "./functions/index.ts"
      ]
    },
    "lib": [
      "esnext",
      "dom"
    ]
  },
  "include": [
    "./*.ts",
    ".stacks/*.js",
    ".stacks/*.ts",
    ".stacks/**/*.js",
    ".stacks/**/*.ts",
    "./.stacks/**/*.js",
    "./.stacks/**/*.ts",
    "./apps/**/*.ts",
    "./apps/**/*.vue",
    "./apps/**/*.json",
    "./components/**/*.json",
    "./components/**/*.vue",
    "./components/**/*.ts",
    "./config/*.ts",
    "./config/*.json",
    "./functions/**/*.ts",
    "./functions/**/*.json"
  ],
  "exclude": [
    "**/dist/**",
    "**/node_modules/**"
  ]
}
