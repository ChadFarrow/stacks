{
  "name": "stacks-docs",
  "type": "module",
  "version": "0.51.0",
  "private": true,
  "description": "The Stacks documentation.",
  "author": "Chris Breuer",
  "license": "MIT",
  "funding": "https://github.com/sponsors/chrisbbreuer",
  "homepage": "https://github.com/stacksjs/stacks/tree/main/.stacks#readme",
  "repository": {
    "type": "git",
