    "copy-src": "rimraf ./src/* && cp -R ../../docs/* ./src"
  },
  "dependencies": {
    "@stacksjs/docs": "link:@stacksjs/docs",
    "@stacksjs/path": "link:@stacksjs/path",
    "rimraf": "^5.0.1",
