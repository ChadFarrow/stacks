{
  "name": "@stacksjs/lint",
  "type": "module",
  "version": "0.57.4",
  "description": "The Stacks way to lint.",
  "author": "Chris Breuer",
  "license": "MIT",
  "funding": "https://github.com/sponsors/chrisbbreuer",
  "homepage": "https://github.com/stacksjs/stacks/tree/main/.stacks/core/utils/lint#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stacksjs/stacks.git",
    "directory": "./.stacks/core/utils/lint"
  },
  "bugs": {
    "url": "https://github.com/stacksjs/stacks/issues"
  },
  "keywords": [
    "linter",
    "formatter",
    "eslint",
    "pre-configured",
    "zero-config",
    "stacks"
  ],
  "exports": {
    ".": {
      "bun": "./src/index.ts"
    },
    "./*": {
      "bun": "./*"
    }
  },
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "contributors": [
    "Chris Breuer <chris@ow3.org>"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "bun --bun build.ts",
    "typecheck": "bun --bun tsc --noEmit",
    "prepublishOnly": "bun --bun run build"
  },
  "peerDependencies": {
    "@ow3/eslint-config": "^0.47.1",
    "@types/eslint": "^8.44.0",
    "eslint": "^8.45.0",
    "lint-staged": "^13.2.3",
    "publint": "^0.1.16"
  },
  "dependencies": {
    "@ow3/eslint-config": "^0.47.1",
    "@types/eslint": "^8.44.0",
    "eslint": "^8.45.0",
    "lint-staged": "^13.2.3",
    "publint": "^0.1.16"
  },
  "devDependencies": {
    "@stacksjs/development": "workspace:*",
    "eslint-plugin-import": "^2.27.5"
  }
}
