  "license": "MIT",
  "funding": "https://github.com/sponsors/chrisbbreuer",
  "homepage": "https://github.com/stacksjs/stacks/tree/main/.stacks/core/cli#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stacksjs/stacks.git",
    "directory": "./.stacks/core/cli"
  },
  "bugs": {
    "url": "https://github.com/stacksjs/stacks/issues"
  },
  "keywords": [
    "cli",
    "commands",
    "command line interface",
    "console",
    "binary",
    "apps",
    "spinners",
    "utilities",
    "helpers",
    "cac",
    "ora",
    "ez-spawn",
    "stacks"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "contributors": [
    "Chris Breuer <chris@ow3.org>"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "bun build ./src/index.ts --outdir dist --format esm --external bun --external cac --external ora --external prompts --external @antfu/install-pkg --external @stacksjs/config --external @stacksjs/error-handling --external @stacksjs/logging --external @stacksjs/path --external @stacksjs/types --external @stacksjs/utils --external @types/prompts --external bun --external cac --external ora --external prompts --external vite --external kolorist --target bun",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "bun run build"
  },
  "peerDependencies": {
    "@antfu/install-pkg": "^0.1.1",
    "@stacksjs/config": "link:@stacksjs/config",
    "@stacksjs/error-handling": "link:@stacksjs/error-handling",
