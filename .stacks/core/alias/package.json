{
  "name": "@stacksjs/alias",
  "type": "module",
  "version": "0.57.4",
  "packageManager": "pnpm@8.6.7",
  "description": "The Stacks aliases.",
  "author": "Chris Breuer",
  "license": "MIT",
  "funding": "https://github.com/sponsors/chrisbbreuer",
  "homepage": "https://github.com/stacksjs/stacks/tree/main/.stacks/core/alias#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stacksjs/stacks.git",
    "directory": "./.stacks/core/alias"
  },
  "bugs": {
    "url": "https://github.com/stacksjs/stacks/issues"
  },
  "keywords": [
    "alias",
    "stacks"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    }
  },
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "contributors": [
    "Chris Breuer <chris@ow3.org>"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "bun build ./src/index.ts --external @stacksjs/path --outdir dist --format esm",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@stacksjs/path": "link:@stacksjs/path"
  },
  "dependencies": {
    "@stacksjs/path": "link:@stacksjs/path"
  },
  "devDependencies": {
    "@stacksjs/development": "link:@stacksjs/development"
  }
}
