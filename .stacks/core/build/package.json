{
  "name": "@stacksjs/build",
  "type": "module",
  "version": "0.57.4",
  "description": "The Stacks framework build tools and configurations.",
  "author": "Chris Breuer",
  "license": "MIT",
  "funding": "https://github.com/sponsors/chrisbbreuer",
  "homepage": "https://github.com/stacksjs/stacks/tree/main/.stacks/core/build#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stacksjs/stacks.git",
    "directory": "./.stacks/core/build"
  },
  "bugs": {
    "url": "https://github.com/stacksjs/stacks/issues"
  },
  "keywords": [
    "build",
    "stacks",
    "framework",
    "typescript",
    "javascript",
    "vite",
    "nitro",
    "vue"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "contributors": [
    "Chris Breuer <chris@ow3.org>"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "bun build ./src/index.ts --external bun --external @stacksjs/vite --outdir dist --format esm",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "bun run build"
  },
  "peerDependencies": {
    "@stacksjs/vite": "link:@stacksjs/vite",
    "bun": "^0.6.14",
    "vue-docgen-web-types": "^0.1.8"
  },
  "dependencies": {
    "@stacksjs/vite": "link:@stacksjs/vite",
    "bun": "^0.6.14",
    "vue-docgen-web-types": "^0.1.8"
  },
  "devDependencies": {
    "@stacksjs/development": "link:@stacksjs/development",
    "vite-plugin-mkcert": "^1.16.0"
  }
}
