{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "useDefineForClassFields": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "strict": true,
    "jsx": "preserve",
    "sourceMap": true,
    "resolveJsonModule": true,
    "importHelpers": true,
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "isolatedModules": true,
    "strictNullChecks": true,
    "baseUrl": ".",
    "outDir": "./dist/types",
    "paths": {
      "framework/*": [
        "./*"
      ],
      "actions/*": [
        "./core/actions/src/*"
      ],
      "build": [
        "./core/build/src/index.ts"
      ],
      "build/*": [
        "./core/build/src/*"
      ],
      "cli": [
        "./core/cli/src/index.ts"
      ],
      "cli/*": [
        "./core/cli/src/*"
      ],
      "helpers": [
        "./core/utils/src/helpers.ts"
      ],
      "modules/*": [
        "./core/modules/src/*"
      ],
      "router": [
        "./core/router/src/index.ts"
      ],
      "router/*": [
        "./core/router/src/*"
      ],
      "security": [
        "./core/security/src/index.ts"
      ],
      "types": [
        "./core/types/src/index.ts"
      ],
      "ui": [
        "./core/ui/src/index.ts"
      ],
      "arrays": [
        "./core/utils/arrays/src/index.tss"
      ],
      "collections": [
        "./core/utils/collections/src/index.tss"
      ],
      "stacks:fs": [
        "./core/utils/fs/src/index.tss"
      ],
      "objects": [
        "./core/utils/objects/src/index.tss"
      ],
      "objects/*": [
        "./core/utils/objects/src/**"
      ],
      "paths": [
        "./core/utils/paths/src/index.tss"
      ],
      "paths/*": [
        "./core/utils/paths/src/**"
      ],
      "strings": [
        "./core/utils/strings/src/index.tss"
      ],
      "utils": [
        "./core/utils/src/index.ts"
      ],
      "stacks": [
        "stacks/src/index.ts"
      ],
      "stacks/*": [
        "stacks/src/*"
      ],
      "components/*": [
        "../components/*"
      ],
      "config": [
        "./core/config/src/index.ts"
      ],
      "config/*": [
        "../config/*"
      ],
      "functions/*": [
        "../functions/*"
      ],
      "pages/*": [
        "../pages/*"
      ],
      "~/*": [
        "../*"
      ]
    },
    "lib": [
      "ESNext",
      "DOM"
    ]
  },
  "include": [
    "./**.d.ts",
    "./**/*",
    "../components/**/*",
    "../functions/**/*",
    "../pages/**/*"
  ],
  "exclude": [
    "./examples/*",
    "./nitro.config.ts",
    "./vitest.config.ts",
    "../.github/commitlint.config.cjs",
    "../**/README.md",
    "../**/dist/**",
    "../**/node_modules/**"
  ]
}
